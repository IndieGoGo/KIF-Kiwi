FRAMEWORK_SEARCH_PATHS = $(inherited) $(PLATFORM_DIR)/Developer/Library/Frameworks "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" "${PODS_ROOT}/Headers/Public/KIF" "${PODS_ROOT}/Headers/Public/KIF-Kiwi" "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" -isystem "${PODS_ROOT}/Headers/Public/KIF" -isystem "${PODS_ROOT}/Headers/Public/KIF-Kiwi" -isystem "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_LDFLAGS = $(inherited) -ObjC -l"KIF" -l"KIF-Kiwi" -l"Kiwi" -framework "CoreGraphics" -framework "IOKit" -framework "QuartzCore" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods